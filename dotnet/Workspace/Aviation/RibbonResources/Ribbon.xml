<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns='http://schemas.microsoft.com/office/2009/07/customui' loadImage='LoadImage' onLoad='OnLoad'>
	<ribbon>
		<tabs>
			<tab id="aviationTab" label="Aviation">
				<group id="generalGroup" label="General">
					<labelControl id="userLabel" getLabel="GetUserLabel" />
					<button id="authentication" getLabel="GetAuthenticateLabel" onAction="OnAuthenticate"/>
					<separator/>
					<button id="save" onAction="OnClick" label="Save" imageMso="FileSave" size="large" getEnabled="GetEnabled"/>
					<button id="refresh" onAction="OnClick" label="Refresh" imageMso="Refresh" size="large" getEnabled="GetEnabled"/>
				</group>
				<group id="salesGroup" label="Sales">
					<button id="customers" onAction="OnClick" label="Customers" getEnabled="GetEnabled"/>
					<button id="salesInvoices" onAction="OnClick" label="Invoices" getEnabled="GetEnabled"/>
				</group>
				<group id="purchasesGroup" label="Purchases">
					<button id="purchaseInvoices" onAction="OnClick" label="Invoices" getEnabled="GetEnabled"/>
				</group>
				<group id="inventoryGroup" label="Operations">
					<button id="serialisedItems" onAction="OnClick" label="All GSE items" getEnabled="GetEnabled"/>
					<button id="rentals" onAction="OnClick" label="Rentals" getEnabled="GetEnabled"/>
					<button id="spareParts" onAction="OnClick" label="Spare Parts" getEnabled="GetEnabled"/>
					<button id="productCategories" onAction="OnClick" label="Product Categories" getEnabled="GetEnabled"/>
					<button id="inventoryTransactions" onAction="OnClick" label="Inventory Transactions" getEnabled="GetEnabled"/>
				</group>
				<group id="workOrderGroup" label="WorkOrders">
					<button id="workOrders" onAction="OnClick" label="WorkOrders" getEnabled="GetEnabled"/>
					<button id="timeEntries" onAction="OnClick" label="TimeEntries" getEnabled="GetEnabled"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
