<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns='http://schemas.microsoft.com/office/2009/07/customui' loadImage='LoadImage' onLoad='OnLoad'>
	<ribbon>
		<tabs>
			<tab id="kickstartTab" label="Kickstart">
				<group id="generalGroup" label="General">
					<labelControl id="userLabel" getLabel="GetUserLabel" />
					<button id="authentication" getLabel="GetAuthenticateLabel" onAction="OnAuthenticate"/>
					<separator/>
					<button id="save" onAction="OnClick" label="Save" imageMso="FileSave" size="large" getEnabled="GetEnabled"/>
					<button id="refresh" onAction="OnClick" label="Refresh" imageMso="Refresh" size="large" getEnabled="GetEnabled"/>
				</group>
				<group id="customGroup" label="Sales">
					<button id="people" onAction="OnClick" label="Customers" getEnabled="GetEnabled"/>
					<button id="medias" onAction="OnClick" label="Invoices" getEnabled="GetEnabled"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
